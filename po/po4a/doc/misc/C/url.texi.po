msgstr ""

#. type: cindex
#: ../../../../../doc/misc/url.texi:521
#, no-wrap
msgid "DAV"
msgstr ""

#. type: table
#: ../../../../../doc/misc/url.texi:524
msgid "A list of numbers specifying what DAV protocol/schema versions are supported."
