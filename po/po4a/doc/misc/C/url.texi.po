#: ../../../../../doc/misc/url.texi:743
#, no-wrap
msgid "data:@r{[}@var{media-type}@r{]}@r{[};@var{base64}@r{]},@var{data}\n"
msgstr ""

#. type: Plain text
#: ../../../../../doc/misc/url.texi:751
msgid "@var{media-type} is a MIME @samp{Content-Type} string, possibly including parameters.  It defaults to @samp{text/plain;charset=US-ASCII}.  The @samp{text/plain} can be omitted but the charset parameter supplied.  If @samp{;base64} is present, the @var{data} are base64-encoded."
