#. type: item
#: ../../../../../doc/misc/url.texi:525
#, no-wrap
msgid "dasl"
msgstr ""

#. type: cindex
#: ../../../../../doc/misc/url.texi:526
#, no-wrap
msgid "DASL"
msgstr ""

#. type: table
