#. type: example
#: ../../../../../doc/misc/efaq-w32.texi:2103
#, no-wrap
msgid ""
"        if (-e \"/dev/tty\") @{\n"
"            $console = \"/dev/tty\";\n"
"            $rcfile=\".perldb\";\n"
"        @}\n"
"        elsif (-e \"con\") @{\n"
"            $console = \"\";                 <---- change \"con\" to \"\"\n"
"            $rcfile=\"perldb.ini\";\n"
"        @}\n"
"        else @{\n"
"            $console = \"sys\\$command\";\n"
"            $rcfile=\"perldb.ini\";\n"
"        @}\n"
msgstr ""

