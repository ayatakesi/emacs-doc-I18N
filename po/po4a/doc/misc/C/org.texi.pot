#, no-wrap
msgid "org-agenda-max-effort"
msgstr ""

#. type: table
#: ../../../../../doc/misc/org.texi:10073
msgid "Limit the duration of accumulated efforts (as minutes)."
msgstr ""

#. type: vindex
#: ../../../../../doc/misc/org.texi:10074 ../../../../../doc/misc/org.texi:10075
#, no-wrap
msgid "org-agenda-max-todos"
msgstr ""

