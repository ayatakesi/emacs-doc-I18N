msgid "treeN"
msgstr ""

#. type: multitable
#: ../../../../../doc/misc/org.texi:7336
msgid "the surrounding level N tree, for example @samp{tree3}"
msgstr ""

#. type: code{#1}
#: ../../../../../doc/misc/org.texi:7336 ../../../../../doc/misc/org.texi:22086
#, no-wrap
msgid "tree"
msgstr ""

