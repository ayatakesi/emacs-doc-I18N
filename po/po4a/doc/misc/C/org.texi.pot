#. type: Plain text
#: ../../../../../doc/misc/org.texi:21935
msgid ""
"Here is an example of a post-processing script in Perl.  It takes the CSV "
"output from Emacs and prints with a checkbox:"
msgstr ""

#. type: example
#: ../../../../../doc/misc/org.texi:21938
#, no-wrap
msgid ""
"#!/usr/bin/perl\n"
"\n"
msgstr ""

#. type: example
#: ../../../../../doc/misc/org.texi:21941
#, no-wrap
msgid ""
"# define the Emacs command to run\n"
"$cmd = \"emacs -batch -l ~/.emacs -eval '(org-batch-agenda-csv "
"\\\"t\\\")'\";\n"
"\n"
msgstr ""

#. type: example
#: ../../../../../doc/misc/org.texi:21944
#, no-wrap
msgid ""
"# run it and capture the output\n"
"$agenda = qx@{$cmd 2>/dev/null@};\n"
"\n"
msgstr ""

#. type: example
#: ../../../../../doc/misc/org.texi:21953
#, no-wrap
msgid ""
"# loop over all lines\n"
"foreach $line (split(/\\n/,$agenda)) @{\n"
"    # get the individual values\n"
"    ($category,$head,$type,$todo,$tags,$date,$time,$extra,\n"
"     $priority_l,$priority_n) = split(/,/,$line);\n"
"    # process and print\n"
"    print \"[ ] $head\\n\";\n"
"@}\n"
msgstr ""

