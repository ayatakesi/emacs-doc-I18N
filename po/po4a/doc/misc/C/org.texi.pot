#: ../../../../../doc/misc/org.texi:20435
#, no-wrap
msgid "org-link-shell-confirm-function"
msgstr ""

#. type: defopt
#: ../../../../../doc/misc/org.texi:20437
msgid "Function that prompts the user before executing a shell link."
msgstr ""

#. type: defopt
#: ../../../../../doc/misc/org.texi:20439
#, no-wrap
msgid "org-link-elisp-confirm-function"
msgstr ""

#. type: defopt
#: ../../../../../doc/misc/org.texi:20441
msgid "Function that prompts the user before executing an Emacs Lisp link."
msgstr ""

