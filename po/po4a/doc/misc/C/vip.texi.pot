msgid "I"
msgstr ""

#. type: kindex
#: ../../../../../doc/misc/vip.texi:1630
#, no-wrap
msgid "111 I @r{(}@code{vip-Insert}@r{)}"
msgstr ""

#. type: table
#: ../../../../../doc/misc/vip.texi:1633
